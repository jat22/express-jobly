{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/jobNew.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "salary": {
      "type": "integer"
    },
    "equity": {
      "type": "number",
      "maximum": 1
    },
    "companyHandle": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "companyHandle"
  ]
}
